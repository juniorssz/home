    <item>
      <title>Introdução</title>
      <link>/home/classes/r-fundamentals/intro/</link>
      <pubDate>Thu, 23 Apr 2020 00:00:00 +0000</pubDate>
      <guid>/home/classes/r-fundamentals/intro/</guid>
